{
  "name": "init-cli",
  "description": "A Vue.js cli",
  "version": "1.0.0",
  "author": "<kuangmiaolong@newbeescm.com>",
  "license": "MIT",
  "private": true,
  "config": {
    "env": "cross-env",
    "mobile": "IS_MOBILE=true",
    "pc": "IS_MOBILE=false",
    "dev": "__DEV__=dev",
    "devpub": "__DEV__=devpub",
    "test": "__DEV__=test",
    "pre": "__DEV__=pre",
    "prod": "__DEV__=prod",
    "port": ""
  },
  "scripts": {
    "init": "yarn config set registry https://registry.npm.taobao.org && yarn config set disturl https://npm.taobao.org/dist",
    "dll": "cross-env webpack -p --config build/webpack.config.dll.js --progress --profile --colors",
    "dev": "cross-env NODE_ENV=development __DEV__=dev IS_MOBILE=true webpack-dev-server --config build/webpack.config.dev.js --open --hot --inline --mode development",
    "devpub": "cross-env __DEV__=devpub IS_MOBILE=true webpack --progress --config build/webpack.config.prod.js --hide-modules --mode production",
    "test": "cross-env __DEV__=test IS_MOBILE=true webpack --progress --config build/webpack.config.prod.js --hide-modules --mode production",
    "pre": "cross-env __DEV__=pre IS_MOBILE=true webpack --progress --config build/webpack.config.prod.js --hide-modules --mode production",
    "prod": "cross-env __DEV__=prod IS_MOBILE=true webpack --progress --config build/webpack.config.prod.js --hide-modules --mode production",
    "devpub:pc": "cross-env ${npm_package_config_devpub} ${npm_package_config_pc} webpack --progress --config build/webpack.config.prod.js --hide-modules",
    "dev:pc": "cross-env ${npm_package_config_dev} ${npm_package_config_pc} webpack-dev-server --config build/webpack.config.dev.js --open --hot --inline --host 0.0.0.0",
    "test:pc": "cross-env ${npm_package_config_test} ${npm_package_config_pc} webpack --progress --config build/webpack.config.prod.js --hide-modules",
    "prod:pc": "cross-env ${npm_package_config_prod} ${npm_package_config_pc} webpack --progress --config build/webpack.config.prod.js --hide-modules",
    "lint": "eslint --ext .js --ext .vue src/",
    "lint-fix": "eslint --fix --ext .js --ext .vue src/"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ],
  "dependencies": {
    "animate.css": "^3.7.0",
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "better-scroll": "^1.12.6",
    "fastclick": "^1.0.6",
    "font-awesome": "^4.7.0",
    "jscalc": "0.0.4",
    "vue": "^2.4.4",
    "vue-color": "^2.4.6",
    "vue-data-loading": "^0.2.4",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vux": "^2.9.2",
    "weixin-js-sdk": "^1.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.1.6",
    "cross-env": "^5.0.5",
    "css-loader": "^1.0.0",
    "es6-promise": "^4.2.5",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.4",
    "happypack": "^5.0.0-beta.4",
    "html-webpack-event-plugin": "^0.2.0",
    "html-webpack-include-assets-plugin": "^1.0.4",
    "html-webpack-plugin": "^3.2.0",
    "inline-manifest-webpack-plugin": "^4.0.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "os": "^0.1.1",
    "postcss-loader": "^2.0.9",
    "postcss-plugin-px2rem": "^0.7.0",
    "postcss-px-to-viewport": "^0.0.3",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^0.6.2",
    "vue-loader": "14.2.3",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.4.4",
    "vux-loader": "^1.2.9",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-merge": "^4.1.1",
    "yaml-loader": "^0.5.0",
    "yarn": "^1.9.4"
  },
  "sideEffects": false
}
